{
    "name": "devisephp/cms",
    "description": "Content Management System using the Laravel Framework",
    "keywords": ["cms", "devise", "devisephp", "laravel", "content management system"],
    "license": "MIT",
    "authors": [
        {
            "name": "DevisePHP Team",
            "email": "info@devisephp.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "^2.7",
        "guzzlehttp/guzzle": "~6.0",
        "laravel/framework": "^7.24",
        "kg-bot/laravel-localization-to-vue": "^1.6",
        "league/glide-laravel": "^1.0",
        "spatie/laravel-glide": "^3.5",
        "spatie/laravel-image-optimizer": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Devise\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devise\\Providers\\DeviseServiceProvider"
            ]
        }
    }
}
